{
  "name": "prevent-file-change",
  "version": "1.3.1",
  "private": true,
  "description": "Fail a pull request workflow if certain files are changed",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write src __tests__ *.js *.json",
    "format-check": "prettier --check src __tests__ *.js *.json",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xalvarez/prevent-file-change-action.git"
  },
  "keywords": [
    "github-actions",
    "node"
  ],
  "author": "Xavier Alvarez",
  "license": "MIT",
  "engines": {
    "node": "16.x"
  },
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.4",
    "@types/node": "^16.18.44",
    "@typescript-eslint/parser": "^5.62.0",
    "@vercel/ncc": "^0.36.1",
    "eslint": "^8.47.0",
    "eslint-plugin-github": "^4.9.2",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.6.4",
    "jest-circus": "^29.5.0",
    "js-yaml": "^4.1.0",
    "prettier": "^3.0.2",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  }
}
